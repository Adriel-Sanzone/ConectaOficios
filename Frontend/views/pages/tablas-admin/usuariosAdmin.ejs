<!DOCTYPE html>
<html lang="en">

<head>
  <%- include ('../../partials/head') %>
  <link rel="stylesheet" href="/Frontend/css/admin.css">
</head>

<body>
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-3">
        <div class="aside">
          <a href="/admin"><i class="fa-solid fa-house-chimney"></i>Inicio</a>
          <p id="toggleButton"><i class="fa-solid fa-server"></i>Base de datos<i class="fas fa-chevron-down abajo"></i></p>
          <ul class="lista-secundaria" style="display: none;">
            <li><a href="categorias">Categorías</a></li>
            <li><a href="especializaciones">Especializaciones</a></li>
            <li><a href="especializacionDeUsuario">Especializacion de Usuario</a></li>
            <li><a href="proyectos">Proyectos</a></li>
            <li><a href="usuariosAdmin">Usuarios</a></li>
          </ul>
        </div>
        <div class="aside-usuario">
          <a href="/index"><i class="fa-solid fa-arrow-left"></i>Cerrar Sesión</a>
          <img class="img-fluid" src="/Frontend/uploads/fede.png" alt="">
        </div>
      </div>

      <div class="col-md-9">
        <div class="head-tablita">
          <p class="title">Tabla Usuarios</p>
        </div>
        <div class="tablita">
          <table>
            <thead>
              <tr>
                <!-- <th class="agregar"></th>
                <th class="editar"></th>
                <th class="eliminar"></th> -->
                <th class="">Id</th>
                <th class="">Email</th>
                <th class="">Password</th>
                <th class="">Nombre</th>
                <th class="">Apellido</th>
                <th class="">Contacto</th>
                <!-- <th class="">Descripcion</th> -->
                <th class="">Destacado</th>
                <th class="">Especialista</th>
                <th class="">Direccion</th>
                <th class="">Token</th>
                <th class="">Path</th>
                <th class="">Path_portada</th>
              </tr>
            </thead>
            <tbody>
              <% usuariosAdmin.forEach(function(usuariosAdmin) { %>
                <tr>
                  <!-- <td class="agregar"></td>
                  <td class="editar"></td>
                  <td class="eliminar"></td> -->
                  <td><%= usuariosAdmin.id %></td>
                  <td><%= usuariosAdmin.email %></td>
                  <td><%= usuariosAdmin.password %></td>
                  <td><%= usuariosAdmin.nombre %></td>
                  <td><%= usuariosAdmin.apellido %></td>
                  <td><%= usuariosAdmin.contacto %></td>
                  <!-- <td><%= usuariosAdmin.descripcion %></td> lo comento porque se hacia mucho texto--> 
                  <td><%= usuariosAdmin.destacado %></td>
                  <td><%= usuariosAdmin.especialista %></td>
                  <td><%= usuariosAdmin.direccion %></td>
                  <td><%= usuariosAdmin.token %></td>
                  <td><%= usuariosAdmin.path %></td>
                  <td><%= usuariosAdmin.path_portada %></td>

                </tr>
              <% }); %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  </div>
  <script src="/Frontend/js/script.js"></script>
</body>

</html>